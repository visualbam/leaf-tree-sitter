{
    "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
    "grammars": [
        {
            "name": "leaf",
            "camelcase": "Leaf",
            "title": "Leaf",
            "scope": "source.leaf",
            "path": ".",
            "file-types": [
                "leaf"
            ],
            "highlights": "queries/highlights.scm",
            "injections": "queries/injections.scm",
            "injection-regex": "^leaf$",
            "class-name": "TreeSitterLeaf"
        }
    ],
    "metadata": {
        "version": "0.1.0",
        "license": "MIT",
        "description": "Leaf template support for tree-sitter",
        "authors": [
            {
                "name": "Bruce McElroy",
                "email": "bruce.mcelroy@outlook.com",
                "url": "https://www.withbruce.com/"
            }
        ],
        "links": {
            "repository": "https://github.com/visualbam/tree-sitter-leaf"
        }
    },
    "bindings": {
        "c": true,
        "go": true,
        "node": true,
        "python": true,
        "rust": true,
        "swift": true,
        "zig": false
    }
}